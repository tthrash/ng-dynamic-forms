{
  "name": "ng-dynamic-forms2",
  "version": "12.4.0",
  "description": "A rapid form development library for Angular",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tthrash/ng-dynamic-forms.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tthrash/ng-dynamic-forms/issues"
  },
  "homepage": "https://github.com/tthrash/ng-dynamic-forms#readme",
  "schematics": "./schematics/collection.json",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:sample": "ng build ng-dynamic-forms --configuration production --aot --base-href http://ng2-dynamic-forms.udos86.de/sample/",
    "build:lib": "npm run build:core && npm run build:ui-basic && npm run build:ui-bootstrap && npm run build:ui-foundation && npm run build:ui-ionic && npm run build:ui-kendo && npm run build:ui-material && npm run build:ui-ng-bootstrap && npm run build:ui-ngx-bootstrap",
    "build:basic-lib": "npm run build:core && npm run build:ui-material",
    "build:core": "ng build @ng-dynamic-forms/core --configuration production",
    "build:ui-basic": "ng build @ng-dynamic-forms/ui-basic --configuration production",
    "build:ui-bootstrap": "ng build @ng-dynamic-forms/ui-bootstrap --configuration production",
    "build:ui-foundation": "ng build @ng-dynamic-forms/ui-foundation --configuration production",
    "build:ui-ionic": "ng build @ng-dynamic-forms/ui-ionic --configuration production",
    "build:ui-kendo": "ng build @ng-dynamic-forms/ui-kendo --configuration production",
    "build:ui-material": "ng build @ng-dynamic-forms/ui-material --configuration production",
    "build:ui-ng-bootstrap": "ng build @ng-dynamic-forms/ui-ng-bootstrap --configuration production",
    "build:ui-ngx-bootstrap": "ng build @ng-dynamic-forms/ui-ngx-bootstrap --configuration production",
    "build:ui-primeng": "ng build @ng-dynamic-forms/ui-primeng --configuration production",
    "build:schematics": "tsc -p ./schematics/tsconfig.json",
    "doc:lib": "compodoc -p projects/tsconfig.compodoc.json -d ./docs",
    "test:lib": "npm run test:core && npm run test:ui-basic && npm run test:ui-bootstrap && npm run test:ui-foundation && npm run test:ui-ionic && npm run test:ui-kendo && npm run test:ui-material && npm run test:ui-ng-bootstrap && npm run test:ui-ngx-bootstrap && npm run test:ui-primeng",
    "test:core": "ng test @ng-dynamic-forms/core --code-coverage",
    "test:ui-basic": "ng test @ng-dynamic-forms/ui-basic --code-coverage",
    "test:ui-bootstrap": "ng test @ng-dynamic-forms/ui-bootstrap --code-coverage",
    "test:ui-foundation": "ng test @ng-dynamic-forms/ui-foundation --code-coverage",
    "test:ui-ionic": "ng test @ng-dynamic-forms/ui-ionic --code-coverage",
    "test:ui-kendo": "ng test @ng-dynamic-forms/ui-kendo --code-coverage",
    "test:ui-material": "ng test @ng-dynamic-forms/ui-material --code-coverage",
    "test:ui-ng-bootstrap": "ng test @ng-dynamic-forms/ui-ng-bootstrap --code-coverage",
    "test:ui-ngx-bootstrap": "ng test @ng-dynamic-forms/ui-ngx-bootstrap --code-coverage",
    "test:ui-primeng": "ng test @ng-dynamic-forms/ui-primeng --code-coverage",
    "update:version": "schematics .:versioning --debug=false",
    "publish": "bash ./publish.sh"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^12.0.4",
    "@angular/cdk": "^12.0.4",
    "@angular/common": "^12.0.4",
    "@angular/compiler": "^12.0.4",
    "@angular/core": "^12.0.4",
    "@angular/forms": "^12.0.4",
    "@angular/material": "^12.0.4",
    "@angular/platform-browser": "^12.0.4",
    "@angular/platform-browser-dynamic": "^12.0.4",
    "@angular/router": "^12.0.4",
    "@ionic/angular": "^5.6.9",
    "@ng-bootstrap/ng-bootstrap": "^9.1.2",
    "@progress/kendo-angular-common": "^2.0.1",
    "@progress/kendo-angular-dateinputs": "^5.2.0",
    "@progress/kendo-angular-dropdowns": "^5.3.0",
    "@progress/kendo-angular-inputs": "^7.3.0",
    "@progress/kendo-angular-intl": "^3.1.0",
    "@progress/kendo-angular-l10n": "^3.0.0",
    "@progress/kendo-angular-popup": "^4.0.0",
    "@progress/kendo-angular-upload": "^7.1.0",
    "@progress/kendo-drawing": "^1.10.1",
    "@progress/kendo-theme-default": "^4.39.0",
    "bootstrap": "^5.0.1",
    "chart.js": "^3.3.2",
    "core-js": "^3.14.0",
    "foundation-sites": "^6.6.3",
    "ngx-bootstrap": "^6.2.0",
    "primeicons": "^4.1.0",
    "primeng": "^11.4.1",
    "quill": "^1.3.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.0.4",
    "@angular-devkit/core": "^12.0.4",
    "@angular-devkit/schematics": "^12.0.4",
    "@angular-devkit/schematics-cli": "^12.0.4",
    "@angular/cli": "^12.0.4",
    "@angular/compiler-cli": "^12.0.4",
    "@angular/language-service": "^12.0.4",
    "ng-packagr": "^12.0.4",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "^3.7.7",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^14.14.25",
    "codelyzer": "^6.0.2",
    "angular2-text-mask": "^9.0.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.0.0",
    "typescript": "~4.2.4"
  }
}
